package org.jhj.myImfo.domain;

public class GameRecordVO {
	private int count;
	private int win;
	private int lose;
	private int draw;
	private int winRate;
	
	public int getCount() {
		return count;
	}
	public void setCount(int count) {
		this.count = count;
	}
	public int getWin() {
		return win;
	}
	public void setWin(int win) {
		this.win = win;
	}
	public int getLose() {
		return lose;
	}
	public void setLose(int lose) {
		this.lose = lose;
	}
	public int getDraw() {
		return draw;
	}
	public void setDraw(int draw) {
		this.draw = draw;
	}
	public int getWinRate() {
		return winRate;
	}
	public void setWinRate() {
		this.winRate = (int)((1/10)*100);
	}
}
